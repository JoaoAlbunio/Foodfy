{% for chef in chefs %}
  <div class="card-chef-admin ">
    <div class="card__avatar-container-admin">
      <div style="background-image: url({{chef.avatar_url}})"></div>
    </div>
    <div class="card__chef-info-admin">
      <h2>{{chef.name}}</h2>
      <a href="/admin/chefs/{{chef.id}}">Visualizar</a>
    </div>
  </div>
{% endfor %}